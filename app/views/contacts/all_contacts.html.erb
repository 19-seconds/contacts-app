<table>
  <tr>
    <td>First Name</td>
    <td>Last Name</td>
    <td>Address</td>
    <td>Phone Number</td>
  </tr>
  <% @all_the_contacts.each do |contact| %>
    <tr>
      <td><%= contact.first_name %></td>
      <td><%= contact.last_name %></td>
      <td><%= contact.address %></td>
      <td><%= contact.phone_number %></td>
    </tr>
  <% end %>

</table>